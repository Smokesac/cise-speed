{
  "version": 2,
  "builds": [
   {
       "src": "*.js",
       "use": "@vercel/node"
   }
  ],
  "routes": [
   {
       "src": "/(.*)",
       "dest": "app.js"
   }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "access-control-allow-origin",
          "value": "*"
        }
      ]
    }
  ]
}
